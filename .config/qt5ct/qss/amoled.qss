QSlider {
    height: 33px;
    width: 200px;
    border: 1px solid palette(text);
    background: palette(base);
}
QSlider::groove {
    background-color: #000b0b;
}
QSlider::handle {
    margin: 2px;
    width: 30px;
    height: 30px;
    border: 1px solid palette(text);
    background-color: palette(highlight);
}
QSlider::handle:hover {
    background-color: palette(text);
}






QLineEdit {
   padding: 7px;
   width: 100px;
    border: 1px solid palette(text);
    color: palette(text);
    background-color: palette(base);
}
QLineEdit[readOnly="true"] { color: gray }
QDialog QLineEdit {
 }


QSpinBox {
    min-height: 30px;
    border: 1px solid palette(text);
    color: palette(text);
    background-color: transparent;     
}
QSpinBox::up-button:hover {
}
QSpinBox::up-button:pressed {
}
QSpinBox::up-arrow {
}


QStatusBar {
    color: #00ff00;
    background-color: #00ff00;
}
QStatusBar::item {
    color: #00ff00;
    background-color: #ff00ff;
}



QProgressBar {
    border-radius: 0px;
    min-height: 30px;
    text-align: center;
    border: 1px solid palette(text);
    color: palette(text);
    background-color: palette(base);
}
QProgressBar::chunk {
    width: 20px;
    color: #00ff00;
    background-color: palette(button);
}





