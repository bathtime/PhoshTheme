QLabel, QToolTip {
    border: 1px solid palette(base);
    color: palette(text);
    background-color: palette(base);
    padding: 0px 6px;
}



QHeaderView {
    padding-left: 4px;
    border: 1px solid palette(base);
    color: palette(text);
    background-color: palette(base);
}
QHeaderView::section {
    padding-left: 4px;
    border: 1px solid palette(base);
    color: palette(text);
    background-color: palette(base);
}
QHeaderView::section:hover {
    padding-left: 4px;
    border: 1px solid palette(base);
    color: palette(highlighted-text);
    background-color: palette(button);
}
QHeaderView::section:checked
{
    background-color: blue;
}

